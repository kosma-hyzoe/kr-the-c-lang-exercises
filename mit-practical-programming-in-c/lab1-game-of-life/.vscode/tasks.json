{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build game of life",
			"command": "/usr/bin/gcc-11",
			"args": [
				"-Wall",
				"-g",
				"-O0",
				"lab1a.c", "lifegame.c",
				"-o",
				"${fileDirname}/live"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc-11"
		},
	]
}